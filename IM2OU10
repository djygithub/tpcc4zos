       LINKAGE SECTION.
       01  IO-PCB.
           03  FILLER            PIC X(10).
           03  IO-STATUS         PIC X(2).
           03  FILLER            PIC X(20).
       PROCEDURE DIVISION.
           ENTRY 'DLITCBL' USING IO-PCB.
       MAIN-ROUTINE.
           MOVE 0 TO UPDATE-COUNTER.
      *(1A)
           EXEC SQL
           SET :UPDATE-VALUE = CURRENT TIMESTAMP
           END-EXEC.
      * GET INPUT MESSAGE
           CALL 'CBLTDLI' USING GET-UNIQUE
                                IO-PCB
                                INPUT-MESSAGE.
      * SEE IF WE GOT ANYTHING
           IF  IO-STATUS NOT = SPACES
               IF  IO-STATUS = 'QC'
                   GOBACK
               ELSE
                   DISPLAY 'ERROR GETTING INPUT MESSAGE', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
      * MOVE INPUT MESSAGE DATA TO SQL AND APPLICATION VARIABLES
           MOVE WH-IN TO WH.
           MOVE DI-IN TO DI.
           MOVE CUSTID-IN TO CUSTID.
           MOVE ARRAY-DEPTH-IN TO ARRAY-DEPTH.
      * DO SQL/DB2 PART
           PERFORM ORDER-STATUS.
      * DO CPU/MEMORY BURNER PART
           MOVE 0 TO NUM-PLANS.
           MOVE 0 TO NUM-TRANS.
           PERFORM PLAN-INIT UNTIL NUM-PLANS > ARRAY-DEPTH.
      * MOVE APPLICATION VARIABLES TO OUTPUT-MESSAGE.
           MOVE TRAN-CODE TO TRANS-CODE.
           MOVE UPDATE-COUNTER TO NUMBER-1.
           MOVE SQLCODE TO NUMBER-2.
      * SEND OUT MESSAGE RESPONSE.
           CALL 'CBLTDLI' USING INSRT
                                IO-PCB
                                OUTPUT-MESSAGE.
      * DID IT WORK
           IF  IO-STATUS NOT = SPACES
                   DISPLAY 'ERROR PUTTING OUTPUT MESSAGE', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
      *
           GO TO MAIN-ROUTINE.
       ORDER-STATUS.
      *(1)
           EXEC SQL
           SELECT MAX(O_ID) INTO :OID FROM DJY00.ORDER10
                  WHERE O_C_ID  = :CUSTID AND
                        O_D_ID  = :DI     AND
                        O_W_ID  = :WH
           END-EXEC.
           IF SQLCODE  = 0
              PERFORM THE-REST-OF-IT
           ELSE
              IF SQLCODE NOT = -305
                   DISPLAY 'ERROR INITIAL QUERY', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.

      *(2)
       THE-REST-OF-IT.
           EXEC SQL
           DECLARE SESAME CURSOR FOR
                 SELECT OL_O_ID,
                        OL_D_ID,
                        OL_W_ID,
                        OL_NUMBER,
                        OL_I_ID,
                        OL_SUPPLY_W_ID,
                        OL_DELIVERY_D,
                        OL_QUANTITY,
                        OL_AMOUNT,
                        OL_DIST_INFO
                 FROM DJY00.ORDERLINE10
                  WHERE OL_O_ID  = :OID  AND
                        OL_D_ID  = :DI AND
                        OL_W_ID  = :WH
                        FOR UPDATE OF OL_DIST_INFO
           END-EXEC.
           IF SQLCODE NOT = 0
                   DISPLAY 'ERROR DECLARE CURSR', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
      *(3)
           EXEC SQL
           SET :UPDATE-VALUE = CURRENT TIMESTAMP
           END-EXEC.
           EXEC SQL
           OPEN SESAME
           END-EXEC.
           IF SQLCODE NOT = 0
                   DISPLAY 'ERROR CLOSE CURSOR', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
      *(4)
           PERFORM STEP-N-FETCHIT UNTIL SQLCODE NOT = 0.

      *(5)
           EXEC SQL
               CLOSE SESAME
           END-EXEC.
           IF SQLCODE NOT = 0
                   DISPLAY 'ERROR CLOSE CURSOR', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.

       STEP-N-FETCHIT.
           EXEC SQL
           FETCH SESAME INTO
              :RET1, :RET2, :RET3, :RET4, :RET5,
              :RET6, :RET7, :RET8, :RET9, :RET10
           END-EXEC.
           IF SQLCODE NOT = 0
              IF SQLCODE NOT = 100
                   DISPLAY 'ERROR FETCH', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
           IF SQLCODE = 0
              EXEC SQL
              UPDATE DJY00.ORDERLINE10
              SET OL_DIST_INFO = :UPDATE-VALUE
              WHERE CURRENT OF SESAME
              END-EXEC
              IF SQLCODE  = 0
                 ADD 1 TO UPDATE-COUNTER
              ELSE
                   DISPLAY 'ERROR UPDATING', IO-PCB,
                            SQLCAID, SQLCABC, SQLCODE, SQLERRM,
                   SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
                   SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6)
                   CALL 'CBLTDLI' USING ROLL.
       SQL-ERROR.
           IF  SQLCODE NOT = 100
           DISPLAY  SQLCAID, SQLCABC, SQLCODE, SQLERRM,
           SQLERRP, SQLWARN, SQLEXT, SQLERRD(1), SQLERRD(2),
           SQLERRD(3), SQLERRD(4), SQLERRD(5), SQLERRD(6).
      * FROM DB2SMF6C START
      ******************************************************************
      *  PLAN-INIT
      ******************************************************************
       PLAN-INIT.
           ADD 1 TO NUM-PLANS.
           ADD 1 TO NUM-TRANS.
           MOVE NUM-PLANS TO PLAN-NUM(NUM-PLANS).
           MOVE 0 TO NUMT(NUM-PLANS).
           MOVE 0 TO  NUMT(NUM-PLANS).
           MOVE 0 TO  ELAPSED-C1(NUM-PLANS).
           MOVE 0 TO  TCB-C1(NUM-PLANS).
           MOVE 0 TO  SRB-C1(NUM-PLANS).
           MOVE 0 TO  ELAPSED-C2(NUM-PLANS).
           MOVE 0 TO  TCB-C2(NUM-PLANS).
           MOVE 0 TO  SRB-C2(NUM-PLANS).
           MOVE 0 TO  LOCKWAIT(NUM-PLANS).
           MOVE 0 TO  IOWAIT(NUM-PLANS).
           MOVE 0 TO  WACN1(NUM-PLANS).
