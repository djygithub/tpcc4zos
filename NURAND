 /********************************************************************/         
 /*                                                                  */         
 /* Module Name = NURAND                                             */         
 /*                                                                  */         
 /* Descriptive Name = non uniform random number generator           */         
 /*                                                                  */         
 /********************************************************************/         
                                                                                
/**********************************************************************/        
/* Include C library definitions                                      */        
/**********************************************************************/        
#include <stdio.h>                                                              
#include <stdlib.h>                                                             
#include <string.h>                                                             
#include <ctest.h>                                                              
  int test;                                                                     
static int rand_integer(val_lo, val_hi)                                         
int val_lo, val_hi;                                                             
{                                                                               
return ((int)(rand() * (val_hi - val_lo + 1)) + val_lo);                        
}                                                                               
static int NUrand_val(A,x,y,C)                                                  
int A, x, y, C;                                                                 
{                                                                               
return ((((rand_integer(0,A) | rand_integer(x,y))+C)%(y-x+1))+x);               
}                                                                               
main()                                                                          
{                                                                               
